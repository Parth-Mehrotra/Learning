<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="paulscode.android.mupen64plus"
      android:versionCode="12"
      android:versionName="1.8.2"
      android:installLocation="auto">
    <application android:label="@string/app_name" android:icon="@drawable/icon" android:launchMode="singleTask">
        <activity android:name="paulscode.android.mupen64plus.MainActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="landscape"
                  android:configChanges="orientation|keyboardHidden|uiMode">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuActivity"
                  android:label="@string/app_name">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsActivity"
                  android:label="Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsVideoActivity"
                  android:label="Video Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsVideoChangeActivity"
                  android:label="Choose Video Plug-in">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsVideoConfigureActivity"
                  android:label="Configure Video Plug-in">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsAudioActivity"
                  android:label="Audio Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsAudioChangeActivity"
                  android:label="Choose Audio Plug-in">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsInputActivity"
                  android:label="Input Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsInputChangeActivity"
                  android:label="Choose Input Plug-in">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsInputConfigureActivity"
                  android:label="Configure Controllers">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsInputConfigureButtonsActivity"
                  android:label="Map Buttons">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsRSPActivity"
                  android:label="RSP Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsRSPChangeActivity"
                  android:label="Choose RSP Plug-in">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsCoreActivity"
                  android:label="Core Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsCoreChangeActivity"
                  android:label="Choose Core">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSettingsCoreConfigureActivity"
                  android:label="Configure Core">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSkinsActivity"
                  android:label="Skins (look and feel)">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSkinsGamepadActivity"
                  android:label="Virtual Gamepad Settings">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.MenuSkinsGamepadChangeActivity"
                  android:label="Select a Layout">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.FileChooserActivity"
                  android:label="@string/app_name">
        </activity>
        <activity android:name="paulscode.android.mupen64plus.SDLActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="landscape"
                  android:configChanges="orientation|keyboardHidden|uiMode">
        </activity>
    </application>
    <uses-feature android:glEsVersion="0x00020000"/>
    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="9"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
</manifest> 
